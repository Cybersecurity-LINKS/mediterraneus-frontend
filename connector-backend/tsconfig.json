{
  "compilerOptions": {
      "lib": ["ES2022", "DOM"],
      "declaration": true,
      "declarationMap": true,
      "target": "ES2022",
      "module": "ESNext",
      "sourceMap": true,
      "strict": false,
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "noImplicitAny": false,
      "removeComments": false,
      "preserveConstEnums": true,
      "outDir": "dist/src",
      "allowSyntheticDefaultImports": true,
      "esModuleInterop": true,
  },
  "include": ["src/**/*"]
}